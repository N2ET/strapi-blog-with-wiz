{
  "routes": [
    {
      "method": "GET",
      "path": "/",
      "handler": "wiz-note-share.index"
    },
    {
      "method": "GET",
      "path": "/config",
      "handler": "wiz-note-share.findConfig"
    },
    {
      "method": "POST",
      "path": "/config",
      "handler": "wiz-note-share.updateConfig"
    },
    {
      "method": "POST",
      "path": "/create_article",
      "handler": "wiz-note-share.create"
    },
    {
      "method": "GET",
      "path": "/articles",
      "handler": "wiz-note-share.find"
    },
    {
      "method": "GET",
      "path": "/articles/:slug",
      "handler": "wiz-note-share.findOneBySlug"
    },
    {
      "method": "GET",
      "path": "/writers",
      "handler": "wiz-note-share.findWriters"
    }
  ]
}
